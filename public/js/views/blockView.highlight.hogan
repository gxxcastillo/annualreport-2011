<img class="img" src="{{img}}" />
<div class="veil">
	{{#label}}
		<div class="c1">{{c1}}</div>
	{{/label}}
	<div class="c2">{{c2}}</div>
	<div class="c3">{{c3}}</div>
</div>

{{! This is only for clickable highlightBlocks}}
{{#isLightbox}}
	<div class="lightboxContent" style="display: none;">
		{{#lightbox}}
			<a href="{{href}}" title="{{title}}" rel="{{lightboxGroup}}"></a>
		{{/lightbox}}
	</div>
{{/isLightbox}}